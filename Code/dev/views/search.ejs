<!DOCTYPE html>
<html>
  <head>
    <%- include ("partials/head.ejs") -%>
  </head>
  <body>
    <div class="container-fluid">
      <div class="col-md-12 py-5 px-3 mx-auto">
        <%- include ("partials/nav.ejs") -%>

        <h1>Search results</h1>

        <% if (message) { %>
        <div class="mb-4 lead"><%= message %></div>
        <% } %>

        <h2>Languages</h2>
        <% languages.forEach(function(item){ %>
        <!--  -->
        <div class="card mb-3" style="max-width: 540px">
          <div class="row no-gutters">
            <div class="col-md-8">
              <div class="card-body">
                <a href="<%= item[2] %>" class="card-link"
                  ><h5 class="card-title"><%=item[2] %></h5></a
                >
                <h6 class="card-subtitle mb-2 text-muted"><%=item[3] %></h6>
                <p class="card-text"><%=item[4] %></p>
                <p class="card-text">
                  <small class="text-muted"><%=item[9] %></small>
                </p>
              </div>
            </div>

            <!-- user -->
            <div class="col-md-4">
              <!-- <img src="..." class="card-img" alt="..." /> -->
              <svg
                xmlns="http://www.w3.org/2000/svg"
                width="50"
                height="50"
                fill="currentColor"
                class="bi bi-person-square"
                viewBox="0 0 16 16"
                style="width: 50px; margin-top: 50px"
              >
                <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0z" />
                <path
                  d="M2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2zm12 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1v-1c0-1-1-4-6-4s-6 3-6 4v1a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h12z"
                />
              </svg>
              <p class="card-text"><%=item[11] %></p>
            </div>
          </div>
          <!--  -->
        </div>
        <% }) %>

